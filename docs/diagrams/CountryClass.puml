@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

package Model {
Class Country
Class CountryNotesManager
Class CountryCodeVerifier
Class Note
Class CountryNote
Class AddressBook
Interface Model <<Interface>>
Class ModelManager
}

CountryNote -up-|> Note
CountryNote -r-> "1" Country
CountryNotesManager "1" -up-> "*" CountryNote
Country .up.> CountryCodeVerifier
AddressBook -left-> "1" CountryNotesManager
ModelManager .up.|> Model
ModelManager -down-> "1" AddressBook

@enduml
